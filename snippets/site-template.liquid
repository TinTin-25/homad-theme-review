          <div class="product-quickview__variants row g-3" style="--bs-gutter-x: 16px; --bs-gutter-y: 16px;">{% raw %}{{{variants}}}{% endraw %}</div>
          <form class="formQuickview form-ajaxtocart mb-4" data-product-form="quickview" action="{{ routes.cart_add_url }}" method="post" accept-charset="UTF-8" enctype="multipart/form-data">
            {% raw %}
              <input type="hidden" data-quickview-variant data-buynow-variant={{ variantID }} name="id" value="{{variantID}}">
            {% endraw %}
            <div class="product-quickview__buttons d-flex row g-2 mb-3">
              <div class="product-quantity col-auto d-flex align-items-center">
                <label class="product-quantity__label d-none">{{ 'products.product.quantity' | t }}:</label>
                <div class="product-quantity__selector">
                  {%- if settings.quickview_quantity_selector == 'text' -%}
                    <div class="vela-qty">
                      <button type="button" class="js-qty-adjust vela-qty__adjust vela-qty__adjust--minus disable" aria-label="{{ 'sections.cart.reduce_quantity' | t }}">
                        {% render 'icons', icon: 'minus' %}
                      </button>
                      <input type="text" name="quantity" class="js-qty-number vela-qty__number" value="1" min="1" aria-label="quantity" pattern="[0-9]*">
                      <button type="button" class="js-qty-adjust vela-qty__adjust vela-qty__adjust--plus" aria-label="{{ 'sections.cart.increase_quantity' | t }}">
                        {% render 'icons', icon: 'plus' %}
                      </button>
                    </div>
                  {%- else -%}
                    <select name="quantity" class="product-quantity__select form-select">
                      {%- for i in (1..10) -%}
                        <option {% if forloop.first %}selected="selected"{% endif %} value="{{ i }}">{{ i }}</option>
                      {%- endfor -%}
                    </select>
                  {%- endif -%}
                </div>
              </div>
              <div class="product-quickview__add-to-cart product-quickview__buttons col">
                <button class="btn btn--add-to-cart" type="submit" name="add">
                  <span class="btn__text">{{ 'products.product.add_to_cart' | t }}</span>
                </button>
