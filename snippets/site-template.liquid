                  <div class="qv-notify-me d-none">
                    <button
                      class="qv-notify-me__btn btn btn-default w-100"
                      type="button"
                      data-bs-toggle="modal"
                      data-bs-target="#qv-notify-popup"
                      data-bs-backdrop="static"
                    ><span class="btn__text">{{ settings.outofstock_title | escape }}</span></button>
                  </div>
                {% endif %}
              </div>
              <div class="col-auto">
                <button
                  type="button"
                  aria-label="{{ 'products.product.wishlist' | t }}"
                  class="js-btn-wishlist product-quickview__btn--wishlist show-tooltip top-0 {% if settings.badge_position == 'start' %}end-0{% else %}start-0{% endif %}"
                  data-product-handle="{% raw %}{{handle}}{% endraw %}"
                >
                    {% render 'icons', icon: 'heart', class: 'heart', attr: 'width="18" height="18"' %}
                    {% render 'icons', icon: 'heart-fill', class: 'heart-fill', attr: 'width="18" height="18"' %}
                  <div class="tooltip bs-tooltip-top" role="tooltip">
                      <div class="tooltip-arrow"></div>
                      <div class="tooltip-inner">
                          {{ 'products.product.add_to_wishlist' | t }}
                      </div>
                  </div>
                </button>
              </div>
            </div>
