<div class="toast ajaxcart-toast fade hide" role="alert" aria-live="assertive" aria-atomic="true" data-delay="2000">
    <button type="button" class="ajaxcart-toast__close text-white btn-link p-0 border-0 top-0 end-0 position-absolute" data-bs-dismiss="toast" aria-label="Close">
      {% render 'icons', icon: 'close-bold' %}
    </button>
  	<div class="toast-body"></div>
</div>
{%- if settings.cart_type == 'drawer' or settings.cart_type == 'modal' -%}
  {%- if settings.pre-order -%}
    <div class="pre_order-cart d-none">
      {% for product in collections.all.products %}
        {%- if product.metafields.custom.pre_order -%}
          {%- assign preOrder = true -%}
        {%- else -%}
          {%- assign preOrder = false -%}
        {%- endif -%}
        {% if preOrder %}
            <span data-handle="{{ product.handle }}"></span>
        {% endif %}
      {% endfor %}
    </div>
  {%- endif -%}
{%- endif -%}
{%- if settings.cart_type == 'drawer' -%}
	<div class="drawer__overlay js-drawer-close"></div>
	<div class="js-drawer drawer drawer--right drawer--has-fixed-footer">
		<div class="drawer__header position-relative">
			<h4 class="drawer__title mb-0">{{ 'sections.cart.title' | t }}<span class="drawer__header__count" data-cart-count>3</span></h4>
			<button type="button" class="drawer__close-button vela__btn-close js-drawer-close">
        {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
        {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
      </button>
      {%- if settings.shipping_bar and settings.shipping_value != blank -%}
				{%- assign shipping_value = settings.shipping_value | times: 100 -%}
				{%- assign price_start = shipping_value | money -%}
				{%- assign spend = shipping_value | minus: cart.total_price -%}
				{%- assign spend = spend | money -%}
				<div class="shipping-bar-cart position-relative pb-md-1 mt-2 lh-1" data-shipping_value="{{ shipping_value }}">
          <div class="title-start">{{ 'sections.cart.free_shipping_start_html' | t: price: price_start }}</div>
          <div class="title-spend">{{ 'sections.cart.free_shipping_spend_html' | t: spend: spend }}</div>
          <div class="title-shipping">{{ 'sections.cart.free_shipping_html' | t }}</div>
          <div class="shipping-progress_bar progress my-2 overflow-visible">
            <div class="progress-bar progress-bar-striped progress-bar-animated position-relative overflow-visible">
              <span class="icon d-flex align-items-center justify-content-center position-absolute rounded-circle top-50 start-100 translate-middle">{%- render 'icons', icon: 'truck-fast', attr: 'width="14"' class: 'svg-shipping' -%}</span>
            </div>
          </div>
				</div>
			{%- endif -%}
		</div>
		<div class="drawer__inner position-absolute overflow-hidden w-100">
			<div class="ajaxcart-container" data-cart-container></div>
		</div>
	</div>
{%- endif -%}
{% unless settings.saleCode_newsletter == 'none' %}
	<div class="js-newsletter-popup adv-popup">
		<div class="js-newsletter-popup-close adv-popup__overlay"></div>
		<div class="adv-popup__inner position-relative overflow-hidden rounded-3" style="--nlt-popup-bg-cl: {{ settings.newsletter_popup_color_bg}};{% if settings.newsletter_width != blank %}--nlt-popup-w: {{ settings.newsletter_width }};{% endif %}{% if settings.newsletter_width_mobile != blank  %}--nlt-popup-w-m: {{ settings.newsletter_width_mobile }};{% endif %}">
      <div class="scroll-style">
        <button
          class="js-newsletter-popup-close adv-popup__close vela__btn-close position-absolute"
          type="button"
          {% if settings.saleCode_newsletter == 'handpicked' %}
            data-newsletter="dontshowagain"
          {% endif %}
          aria-label="{{ 'accessibility.close' | t }}"
        >
          {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
          {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
        </button>
        <div class="adv-popup__content">
          {%- if settings.saleCode_newsletter == 'newsletter' -%}
            <div class="row gx-0 w-100 h-100 align-items-center">
              <div class="newsletter-popup__image col-auto d-none d-md-block" style="--plh-svg-h: 400px;">
                {%- if settings.newsletter_popup_image -%}
                  <img src="{{ settings.newsletter_popup_image | img_url: 'master' }}" loading="lazy" class="img-fluid" alt="{{ settings.newsletter_popup_image.alt | default: shop.name | escape }}" />
                {%- else -%}
                  {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                {%- endif -%}
              </div>
              <div class="newsletter-popup__content col d-flex justify-content-center align-items-center">
                <div class="newsletter-popup__wrapper p-4 py-md-5">
                    {%- if settings.newsletter_popup_title != blank -%}
                      <h4 class="newsletter-popup__title mb-2" style="--nlt-popup-h-cl: {{ settings.newsletter_title_color }};">{{ settings.newsletter_popup_title }}</h4>
                    {%- endif -%}
                    {%- if settings.newsletter_sub_html != blank -%}
                      <div class="newsletter-popup__subTitle mb-4" style="--nlt-popup-subtitle-cl: {{ settings.newsletter_html_color }};">
                        {{ settings.newsletter_sub_html }}
                      </div>
                    {%- endif -%}
                    {%- assign mailchimp = settings.newsletter_popup_mailchimp -%}
                    {%- assign mailchimp_action = settings.newsletter_popup_form_action | replace: '/post?', '/post-json?' | append: '&amp;c=?' -%}
                    {%- if mailchimp -%}
                      <form class="js-vela-newsletter" action="{{ mailchimp_action }}" method="post" name="mc-embedded-subscribe-form" target="_blank">
                        <div class="newsletter-form__wrap">
                          <input
                            class="js-input-newsletter form-control newsletter-form__input"
                            type="email"
                            name="EMAIL"
                            value="{% if customer %}{{ customer.email }}{% endif %}"
                            placeholder="{{ 'newsletter.newsletter_email' | t }}"
                            required
                          />
                          <button class="btn newsletter-form__submit w-100 mt-2" type="submit">
                            <span>{{ 'newsletter.button_label' | t }}</span>
                          </button>
                        </div>
                      </form>
                    {%- else -%}
                      {% form 'customer' %}
                        {{ form.errors | default_errors }}
                        {%- if form.posted_successfully? -%}
                          <div class="js-newsletter-popup-success alert alert-success">{{ 'newsletter.newsletter_confirmation' | t }}</div>
                        {%- else -%}
                          <input type="hidden" name="contact[tags]" value="newsletter">
                          <div class="newsletter-form__wrap flex-wrap">
                            <input
                            value="{% if customer %}{{ customer.email }}{% endif %}"
                            placeholder="{{ 'newsletter.newsletter_email' | t }}"
                            name="contact[email]"
                            class="form-control newsletter-form__input"
                            type="email" 
                            autocorrect="off"
                            autocapitalize="off"/>
                            <button class="btn newsletter-form__submit w-100 mt-2" type="submit">
                              <span>{{ 'newsletter.button_label' | t }}</span>
                            </button>
                          </div>
                        {%- endif -%}
                      {% endform %}
                    {%- endif -%}
                    {%- if settings.newsletter_desc_html != blank -%}
                      <div class="newsletter-popup__desc mt-2" style="--nlt-popup-desc-cl: {{ settings.newsletter_desc_color }};">
                        {{- settings.newsletter_desc_html -}}
                      </div>
                    {%- endif -%}
                    <button data-newsletter="dontshowagain" class="border-0 bg-transparent p-0 mt-3 mt-sm-4 text-decoration-underline text-color" type="button">{{ 'newsletter.no_thank' | t }}</button>
                </div>
              </div>
            </div>
          {%- elsif settings.saleCode_newsletter == 'saleCode' -%}
            {%- capture sc-bg-color -%}
              {%- assign opacity = settings.opacity | times: 0.01 -%}
              {%- assign overlay = settings.saleCode_color_bg | color_modify: 'alpha', opacity -%}
                --sc-ct-bg: {{ overlay }};
            {%- endcapture -%}
            {%- capture sc-bg-image -%}
              {%- if settings.saleCode_image != blank -%}
                  background-image: url({{ settings.saleCode_image | img_url: 'master' }}); background-size: cover; background-repeat: no-repeat;
              {%- endif -%}
            {%- endcapture -%}
            <div class="row g-0 h-100">
              <div class="col-12 col-md-auto" style="{{ sc-bg-image }}">
                <div class="saleCode-popup__form d-flex align-items-center w-100 h-100" style="{{- sc-bg-color -}}">
                  <div class="saleCode-popup__form-wrap text-center w-100 p-4 my-3 my-md-0">
                    {%- if settings.saleCode_title != blank -%}
                      <h2 class="saleCode-popup__title mb-3" style="--sc-t-cl: {{ settings.saleCode_title_color }};">{{ settings.saleCode_title }}</h2>
                    {%- endif -%}
                    {%- if settings.saleCode_code != blank -%}
                      <div class="d-flex w-100 mx-auto position-relative saleCode__input">
                        <input class="form-control fs-13 fw-semibold text-center text-white text-uppercase pe-5" type="text" value="{{ settings.saleCode_code }}" id="saleCode">
                        <button id="saleCodeBtnCopy" class="show-tooltip position-absolute border-0 rounded-circle d-flex align-items-center justify-content-center bg-white end-0 top-0" onclick="copyToClipboard()" data-title-success="{{ 'gift_cards.issued.copy_code_success' | t }}">
                            <svg width="16" height="16" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M13.92 4H2V17H13.92V4Z" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/><path d="M5 1H17V13" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/></svg>
                            <div class="tooltip bs-tooltip-top" role="tooltip">
                                <div class="tooltip-arrow"></div>
                                <div class="tooltip-inner">
                                    {{ 'gift_cards.issued.copy_code' | t }}
                                </div>
                            </div>
                        </button>                                             
                      </div>
                    {%- endif -%}
                    {%- if settings.saleCode_desc_html != blank -%}
                      <div class="saleCode-popup__desc mt-2 mx-auto" style="--sc-dsc-cl: {{ settings.saleCode_desc_color }};">
                        {{- settings.saleCode_desc_html -}}
                      </div>
                    {%- endif -%}
                    <button class="js-newsletter-popup-close saleCode-popup__btn btn bg-white heading-color px-4 mt-4" type="button">
                      {{ settings.saleCode_button_label }}
                    </button>
                    <button data-newsletter="dontshowagain" class="border-0 bg-transparent p-0 mt-3 mt-sm-4 text-decoration-underline text-white" type="button">{{ 'newsletter.no_thank' | t }}</button>
                  </div>
                </div>
              </div>
              <div class="saleCode-popup__content col">
                <div class="saleCode-popup__product p-4">
                  {%- if settings.saleCode_product_title != blank -%}
                    <div class="fw-medium fs-4 mb-3 lh-1 heading-color">{{ settings.saleCode_product_title }}</div>
                  {%- endif -%}
                  <div class="saleCode-popup__product-content">
                    {%- assign proCollection = settings.saleCode_collection -%}
                    {%- if proCollection == blank or collections[proCollection] == empty or collections[proCollection].products_count == 0 -%}
                      {% include 'onboarding-featured-products', product_style: "style1", number: '3' %}
                    {%- else -%}
                      <div class="row g-0" style="--bs-gutter-y: 16px;">
                        {%- for product in collections[proCollection].products limit: settings.saleCode_product_limit -%}
                            {% render 'product-list__style1', product: product %}
                        {%- endfor -%}
                      </div>
                    {%- endif -%}
                    <div class="pt-1 pb-2">
                      <a href="{{ routes.all_products_collection_url }}" class="btn btn-default w-100 mt-3">
                        {{ 'general.continue_shopping' | t }}
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <script>
              function copyToClipboard() {
                  var saleCode = document.getElementById("saleCode");
                  saleCode.select();
                  saleCode.setSelectionRange(0, 99999); 
                  navigator.clipboard.writeText(saleCode.value);
                  var saleCodeTooltip = document.getElementById("saleCodeBtnCopy");
                  let textSuccess = saleCodeTooltip.getAttribute("data-title-success");
                  saleCodeTooltip.setAttribute("title", textSuccess);
                  const tooltipText = saleCodeTooltip.querySelector('.tooltip-inner');
                  if (tooltipText) {
                      tooltipText.innerHTML = textSuccess;
                  }
              }
            </script>
          {% else %}
            <div class="handpicked p-4">
              {%- if settings.handpicked_product_title != blank -%}
                <div class="mb-3 text-center">
                    <h3 class="lh-1 mb-2">{{ settings.handpicked_product_title }}</h3>
                  {%- if settings.handpicked_product_subtitle != blank -%}
                    <div class="fs-6">{{ settings.handpicked_product_subtitle }}</div>
                  {%- endif -%}
                </div>
              {%- endif -%}
              <div class="handpicked__content">
                {%- assign proCollection = settings.handpicked_collection -%}
                <div class="js-carousel slick-carousel row g-3"
                  data-nav="true"
                  data-autoplay="true"
                  data-columnone="4" 
                  data-columntwo="3" 
                  data-columnthree="2" style="--bs-gutter-x: 16px;">
                  {%- if proCollection == blank or collections[proCollection] == empty or collections[proCollection].products_count == 0 -%}
                    {% render 'onboarding-featured-products', slide_variableWidth: false, number: 4 %}
                  {%- else -%}
                    {%- for product in collections[proCollection].products limit: settings.handpicked_product_limit -%}
                      <div class="slick-carousel__item">
                        {% render 'product-grid-item', product: product %}
                      </div>
                    {%- endfor -%}
                  {%- endif -%}
                </div>
              </div>
            </div>
          {% endif %}
        </div>
      </div>
		</div>
	</div>
{%- endunless -%}
{%- if settings.scroll_to_top -%}
		<div class="js-scroll-to-top scroll-to-top">
			<span class="rounded-circle d-flex align-items-center justify-content-center">{% render 'icons', icon: 'arrow_top', attr: 'width="10" height="18"' %}</span>
		</div>
	{%- endif -%}
{%- if settings.quickview_enable -%}
  <div class="quickview modal fade"
    data-quickview
    data-bs-keyboard="false"
    data-stock-show="{{ settings.quickview_availability }}"
    data-type-show="{{ settings.quickview_protype }}"
    data-vendor-show="{{ settings.quickview_vendor }}"
  >
    <div class="quickview__close" data-quickview-close></div>
    <div class="quickview__content modal-dialog modal-dialog-centered">
      <div class="modal-content overflow-hidden">
        <div class="quickview__wrapper">
          <button type="button" class="quickview__btn-close vela__btn-close" data-quickview-close>
            {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
            {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
          </button>
          <div class="quickview__content__inner" data-quickview-container></div>
        </div>
      </div>
    </div>
  </div>
{%- endif -%}
  <div class="quickadd modal fade"
    data-quickadd
    data-stock-show="{{ settings.quickadd_availability }}"
    data-type-show="{{ settings.quickadd_protype }}"
    data-vendor-show="{{ settings.quickadd_vendor }}"
  >
    <div class="quickadd__content modal-dialog modal-dialog-centered">
      <div class="modal-content overflow-hidden">
        <div class="scroll-style">
          <button type="button" class="quickadd__btn-close vela__btn-close" data-bs-dismiss="modal">
            {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
            {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
          </button>
          <div class="quickadd__content__inner" data-quickadd-container></div>
        </div>
      </div>
    </div>
  </div>
{% if settings.outofstock_enable %}
  <div class="modal fade" id="qv-notify-popup" tabindex="-1" aria-labelledby="qv-notify-popup-title" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
      <div class="modal-content">
        {% form 'contact' %}
          <div class="notify-product position-relative border-bottom">
            <button type="button" class="vela__btn-close position-absolute top-0 end-0 mt-3 me-4" data-bs-dismiss="modal" aria-label="Close">
                {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
                {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
            </button>
            <div class="row g-0">
                <div class="notify-product__image position-relative col-auto" data-product-image>
                    {%- render 'preloading', preload_class: 'position-absolute' -%}
                </div>
                <div class="notify-product__content col ps-2 ms-1">
                    <div class="notify-product__title fs-6 heading-color me-5" data-product-title></div>
                    <div class="notify-product__variants mt-2 text-secondary" data-product-options></div>
                    <div class="notify-product__price lh-1 mt-2" data-product-price>
                      <span class="js-price notify-product__price"></span>
                      <span class="js-price-compare notify-product__price notify-product__price--compare"></span>
                    </div>
                </div>
            </div>
          </div>
          <div class="notify-product__form position-relative">
              <input type="hidden" name="contact[title]" value="{{ settings.outofstock_title | escape }}">
              <input type="hidden" name="contact[product_id]" data-input-id value="">
              <input type="hidden" name="contact[variant_id]" data-variant-id value="">
              <input type="hidden" name="contact[product]" data-input-title value="">
              <input type="hidden" name="contact[variant]" data-variant value="">
              <input type="hidden" name="contact[product_url]" data-input-url value="">
              {%- if settings.outofstock_modal_content != blank -%}
                <div class="notify-product__description h4 text-center">{{ settings.outofstock_modal_content }}</div>
              {%- endif -%}
              <input
                class="form-control"
                type="email"
                name="contact[email]"
                value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                placeholder="{{ 'templates.contact.form.email_placeholder' | t }}"
                required
              />
              <button class="btn btn-default w-100" type="submit">{{ settings.outofstock_modal_title }}</button>
          </div>
        {% endform %}
      </div>
    </div>
  </div>
  <div id="product-card-bis" class="bis-notify modal fade" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
      <div class="modal-content oveflow-hidden">
        <div class="scroll-style">
            {% form 'contact' %}
              {%- if form.posted_successfully? -%}
                <p class="alert alert-success">{{ 'templates.contact.bis_notify_success' | t }}</p>
              {%- else -%}
                <div class="bis-notify__content"></div>
              {%- endif -%}
            {% endform %}
        </div>
      </div>
    </div>
  </div>
{% endif %}
{%- render 'vela-swatches' -%}
{%- if settings.cart_type == 'modal' -%}
  <script id="ajaxcart-header-template" type="text/template">
  {% raw %}
    <div class="js-cart-modal cart-modal fade">
      <div class="js-cart-modal-close cart-modal__overlay"></div>
      <div class="cart-modal__inner modal-content row gx-0 m-auto">
  {% endraw %}
        <button type="button" class="js-cart-modal-close cart-modal__close vela__btn-close" href="javascript:void(0)">
          {% render 'icons', icon: 'line', class: 'icon-line position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
          {% render 'icons', icon: 'line', class: 'icon-line1 position-absolute top-50 start-50', attr: 'width="10" height="10"' %}
        </button>
  {% raw %}
		<div class="col-12 col-sm-5">
			<div class="cart-modal__image overflow-hidden me-sm-3 me-lg-4 position-relative">
				<img class="img-fluid position-relative" alt="{{name}}" src="{{image}}"/>
				{% endraw %}
					{% render "preloading", preload_class: "position-absolute" %}
				{% raw %}
			</div>
		</div>
        <div class="cart-modal__content col-12 col-sm-7">
          <div class="cart-modal__name">{{name}}</div>
          {{#if pre_order}}
            <div class="ajaxcart__product-meta text-info pro_preorder mb-1">{% endraw %}{{ 'products.general.status' | t }}: {{ 'products.general.label_pre_order' | t }}{% raw %}</div>
          {{/if}}
          {{#if variation}}
            <div class="ajaxcart__product-meta text-secondary d-flex flex-wrap mb-2">
              {{#each options}}
                <span>{{ this.name }}: {{ this.value }}</span>
              {{/each}}
            </div>
          {{/if}}
          <div class="ajaxcart__product-price-wrapper d-flex product-price mb-2">
            <span class="ajaxcart__price">{{{price}}}</span>
          </div>
  {% endraw %}
          <div class="cart-modal__messenger">{{ 'sections.cart.added_to_cart' | t }}</div>
          <form action="{{ routes.cart_url }}" method="post" class="ajaxcart">
            <div class="mt-2">
              {%- capture terms_conditions -%}
                {%- if settings.terms_conditions != blank -%}
                  {{ 'sections.cart.terms_conditions_html' | t: link: settings.terms_conditions }}
                {%- elsif shop.terms_of_service.body != blank  -%}
                  {{ 'sections.cart.terms_conditions_html' | t: link: shop.terms_of_service.url }}
                {%- else -%}
                  {{ 'sections.cart.terms_conditions_html' | t: link: '#' }}
                {%- endif -%}
              {%- endcapture -%}
              <input class="form-check-input rounded-circle" type="checkbox" value="" id="cart-modal__terms">
              <label class="form-check-label" for="cart-modal__terms">
                {{ terms_conditions }}
              </label>
              <button class="btn btn-default cart-modal__btn cart-modal__btn--checkout mt-2 w-100" type="submit" name="checkout">
                <span>{{ 'sections.cart.checkout' | t }}</span>
              </button>
            </div>
            <div class="cart-modal__funcs">
              <button class="btn btn-outline cart-modal__btn cart-modal__btn--viewcart w-100" type="submit">
                <span>{{ 'sections.cart.viewcart' | t }}</span>
              </button>
            </div>
          </form>
  {% raw %}
        </div>
      </div>
    </div>
  {% endraw %}
  </script>
{%- endif -%}
<script id="ajaxcart-template" type="text/template">
{% raw %}
  <form action="{% endraw %}{{ routes.cart_url }}{% raw %}" method="post" novalidate class="cart ajaxcart ajaxcart--header">
    <div class="ajaxcart__inner scroll-style {% endraw %}{% if settings.cart_type == 'drawer' %}d-flex flex-column pb-3 pt-1 {% if settings.cart_type == 'drawer' %} justify-content-between{% endif %}{% endif %}{% raw %}">
      <div class="ajaxcart__inner--wrapp">
        {{#items}}
        <div class="ajaxcart__product" data-line="{{line}}">
          <div class="ajaxcart__line-item d-flex">
            <div class="ajaxcart__line-item--left me-1 pe-2">
              <div class="ajaxcart__product-image-wrapper position-relative">
                <a href="{{url}}" class="ajaxcart__product-image"><img class="img-fluid" loading="lazy" src="{{img}}" alt="{{name}}"></a>
              </div>
            </div>
            <div class="ajaxcart__line-item--right">
              <div class="ajaxcart__product-description">
                <div class="ajaxcart__product-name-wrapper">
                  <a href="{{url}}" class="ajaxcart__product-name line-clamp-1 mb-2" title="{{{name}}}"><span>{{{name}}}</span></a>
                  <div class="ajaxcart__product-meta text-info pro_preorder mb-1 d-none">{% endraw %}{{ 'products.general.status' | t }}: {{ 'products.general.label_pre_order' | t }}{% raw %}</div>
                  {{#if variation}}
                    <div class="ajaxcart__product-meta text-secondary d-flex flex-wrap mb-2">
                      <div class="ajaxcart__meta-inner d-flex pe-1">
                        {{#each options}}
                          <span>{{ this.value }}</span>
                        {{/each}}
