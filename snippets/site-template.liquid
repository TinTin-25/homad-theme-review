              <label class="form-check-label" for="quickview-terms-condition">
                {{ terms_conditions }}
              </label>
              <div class="vela-buynow__content mt-3">
                <button class="js-button btn btn-buynow" type="button">
                  <span class="btn__text">{{ 'products.product.buy_now' | t }}</span>
                </button>
              </div>
            </vela-buynow>
          </form>
          <div class="product-quickview__organization--wrap">
            {%- if settings.quickview_availability -%}
              <div class="product-quickview__organization product-avaiable product-avaiable--active product-avaiable--instock">
                <div class="product-avaiable__label product-quickview__organization__label">{{ 'products.product.availability' | t }}:</div>
                <div class="js-product-avaiable-text product-avaiable__text product-avaiable__text--instock">{{'products.product.in_stock' | t }}</div>
              </div>
              <div class="product-quickview__organization product-avaiable product-avaiable--outstock">
                <div class="product-avaiable__label product-quickview__organization__label">{{ 'products.product.availability' | t }}:</div>
                <div class="js-product-avaiable-text product-avaiable__text product-avaiable__text--outstock">{{'products.product.out_stock' | t }}</div>
              </div>
            {%- endif -%}
            {%- if settings.quickview_prosku -%}
              <div class="product-quickview__organization">
                <div class="product-quickview__organization__label label_sku">{{ 'products.product.sku' | t }}</div>
                {% raw %}<div class="product-quickview__organization__text" data-quickview-sku>{{sku}}</div>{% endraw %}
              </div>
            {%- endif -%}
            {%- if settings.quickview_protype -%}
              <div class="product-quickview__organization mb-1">
                <div class="product-quickview__organization__label">{{ 'products.product.type' | t }}</div>
                {% raw %}<div class="product-quickview__organization__text">{{{type}}}</div>{% endraw %}
              </div>
            {%- endif -%}
            {%- if settings.quickview_tags -%}
              <div class="product-quickview__organization mb-1">
                <div class="product-quickview__organization__label">{{ 'products.product.tags' | t }}</div>
                {% raw %}<div class="product-quickview__organization__text d-flex flex-wrap">{{{tags}}}</div>{% endraw %}
              </div>
            {%- endif -%}
          </div>
          {%- if settings.quickview_text != blank -%}
            <div class="quickview__btn-viewfull d-flex justify-content-center mt-4">
              <a class="fs-13 fw-medium d-flex align-items-center" {% raw %}href="{{{ url }}}"{% endraw %}>
                {{ settings.quickview_text }}
                <svg class="ms-1" width="16" height="10" viewBox="0 0 16 10" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M10.5 1L15 5L10.5 9" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/><path d="M1.5 5H13.5" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/></svg>
              </a>
            </div>
          {%- endif -%}
        </div>
    </div>
</script>
