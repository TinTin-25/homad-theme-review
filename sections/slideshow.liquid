{%- include 'section_style' -%}
{%- if section.settings.style_special -%}<style>.template-index .section--slideshow{margin-top:calc(-1* var(--header-height, 0px));}.template-index .section-header:not(.scrolled-past-header) .vela-section:not(:hover){background-color:transparent!important;}</style>{%- endif -%}
<div class="vela-section section--slideshow overflow-hidden{% if section.settings.full_with and section.settings.max_with %} mx-auto{% endif %}" style="{{- section_style -}}">
	<div class="container{% if section.settings.full_with %}-full{% endif %}">
		<div class="vela-slideshow__inner position-relative overflow-hidden {{ section.settings.border_radius }}{% if section.settings.caption_max_with != blank %} caption_container{% endif %}" {% if section.settings.caption_max_with != blank %} style="--sl-ct-mx-w: {{ section.settings.caption_max_with }};"{% endif %}>
			<div class="js-vela-slideshow vela-slideshow{% if section.settings.scrolldown %} scroll_down{% endif%}"
				data-autoplay="{{ section.settings.autoplay }}"
				data-autoplayinterval="{{ section.settings.autoplay_speed }}"
				data-navigation="{{ section.settings.navigation }}"
				data-pagination="{{ section.settings.pagination }}">
				{%- for block in section.blocks -%}
					<div class="vela-slideshow__item">
						<a href="{{ block.settings.link }}" class="vela-slideshow__item-link" aria-label="{{ block.settings.title | escape | default: shop.name }}">
							{%- capture image_style -%}
									{%- if section.settings.slide-show-fullheight -%}
										--sl-h: calc(100vh - var(--header-group-height, 0px)); --sl-th: calc(100vh - var(--header-group-height, 0px)); --sl-mh: calc(100vh - var(--header-group-height, 0px)); --plh-svg-h: calc(100vh - var(--header-group-height, 0px));				
									{%- else -%}
										--plh-svg-h: {{ section.settings.slideshow-height }};
										{%- if section.settings.slideshow-height != blank -%}
											--sl-h: {{ section.settings.slideshow-height }};
										{%- else -%}
											--sl-h: {{ block.settings.image.height }};
										{%- endif -%}
											--sl-th: {{ section.settings.slideshow-theight }};
											--sl-mh: {{ section.settings.slideshow-mheight }};
									{%- endif -%}
										background-position: {{ block.settings.bg_align }};
								{%- endcapture -%}
							<div class="slide--image" style="{{ image_style }}">
								{%- if block.settings.image != blank -%}
									<div class="{% if block.settings.image_mobile != blank %}d-none d-md-block{% else %}d-block{% endif %} lazyload"
										data-bgset="{%- render 'bgset', image: block.settings.image -%}"
										data-sizes="auto"
										data-parent-fit="cover">
									</div>
									{%- if block.settings.image_mobile != blank -%}
										<div class="d-block d-md-none lazyload"
											data-bgset="{%- render 'bgset', image: block.settings.image_mobile -%}"
											data-sizes="auto"
											data-parent-fit="cover">
										</div>
									{%- endif -%}
								{%- else -%}
									<div class="placeholder-background">
										{%- capture current -%}{% cycle 1, 2 %}{%- endcapture -%}
										{{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
									</div>
								{%- endif -%}
							</div>
						</a>
						{%- assign time = 0.5 -%}
						{%- capture box_style -%}
							{%- if block.settings.padding_block != blank -%}
								--sl-box-pd-dt: {{ block.settings.padding_block }};
							{%- endif -%}
							{%- if block.settings.padding_block_m != blank -%}
								--sl-box-pd-mb: {{ block.settings.padding_block_m }};
							{%- endif -%}
							{%- if block.settings.box_maxwidth != blank -%}
								--sl-box-mx: {{ block.settings.box_maxwidth }};
							{%- endif -%}
							{%- assign opacity = block.settings.opacity | times: 0.01 -%}
							{%- assign overlay = block.settings.content_color | color_modify: 'alpha', opacity -%}
							--sl-box-bg: {{ overlay }};
						{%- endcapture -%}
						<div class="slideshow-caption {% unless section.settings.position_content %} captionPosition{% endunless %}{% if block.settings.subheading == blank and block.settings.title == blank and block.settings.title2 == blank and block.settings.desc == blank and block.settings.btn_text == blank %} d-none{% endif %}">
							<div class="captionWrap container{% if section.settings.caption_full_with %}-fluid{% endif %} d-flex w-100 h-100 position-relative {{ block.settings.alignment }} {{ block.settings.mb-alignment }}">
								<div class="slideshow-caption__inner" style="{{ box_style }}">
									<div class="slideshow-caption__content text-{{ block.settings.text-mb-alignment }} text-md-{{ block.settings.text-alignment }} ">
										{%- unless block.settings.subheading == blank -%}
											{%- capture text_style -%}
												--vl-t-cl:{{ block.settings.subheading_color }};--vl-t-fs: {{ block.settings.subheading_font_size }}px;--vl-t-fs-tl: {{ block.settings.subheading_font_size_l }}px;--vl-t-fs-m: {{ block.settings.subheading_font_size_m }}px;
												{%- if block.settings.subheading_margin != blank -%}--vl-t-mg: {{ block.settings.subheading_margin }};
												{%- endif -%}{%- if block.settings.subheading_margin_m != blank -%} --vl-t-mg-m: {{ block.settings.subheading_margin_m }};{% endif %}
											{%- endcapture -%}
											<div class="slideshow-caption__subheading vela-text lh-1 {{ block.settings.subheading_font_family }} animate__{{ block.settings.slides_effect }}" style="{{- text_style -}}">
												{{ block.settings.subheading }}
											</div>
											{%- assign time = time | plus: 0.3 -%}
										{%- endunless -%}
										{%- unless block.settings.title == blank -%}
											{%- capture text_style -%}
												--vl-t-cl:{{ block.settings.title_color }};--vl-t-fs: {{ block.settings.title_font_size }}px;--vl-t-fs-tl: {{ block.settings.title_font_size_l }}px;--vl-t-fs-m: {{ block.settings.title_font_size_m }}px;{%- if block.settings.title_margin != blank -%}--vl-t-mg: {{ block.settings.title_margin }};{%- endif -%}{%- if block.settings.title_margin_m != blank -%} --vl-t-mg-m: {{ block.settings.title_margin_m }};{% endif %}
											{%- endcapture -%}
											<div class="slideshow-caption__heading vela-text lh-1 {{ block.settings.title_font_family }} animate__{{ block.settings.slides_effect }}" style="--animate-duration: {{ time }}s;{{- text_style -}}">
												{{ block.settings.title }}
											</div>
											{%- assign time = time | plus: 0.3 -%}
										{%- endunless -%}
										{%- unless block.settings.title2 == blank -%}
											{%- capture text_style -%}
												--vl-t-cl:{{ block.settings.title2_color }};--vl-t-fs: {{ block.settings.title2_font_size }}px;--vl-t-fs-tl: {{ block.settings.title2_font_size_l }}px;--vl-t-fs-m: {{ block.settings.title2_font_size_m }}px;
												{%- if block.settings.title2_margin != blank -%}--vl-t-mg: {{ block.settings.title2_margin }};
												{%- endif -%}{%- if block.settings.title2_margin_m != blank -%} --vl-t-mg-m: {{ block.settings.title2_margin_m }};{% endif %}
											{%- endcapture -%}
											<div class="slideshow-caption__heading--small vela-text lh-1 {{ block.settings.title2_font_family }} animate__{{ block.settings.slides_effect }}" style="--animate-duration: {{ time }}s;{{- text_style -}}">
												{{ block.settings.title2 }}
											</div>
											{%- assign time = time | plus: 0.3 -%}
										{%- endunless -%}
										{%- unless block.settings.desc == blank -%}
											{%- capture text_style -%}
												--vl-t-cl:{{ block.settings.desc_color }};--vl-t-fs: {{ block.settings.desc_font_size }}px;--vl-t-fs-tl: {{ block.settings.desc_font_size_l }}px;--vl-t-fs-m: {{ block.settings.desc_font_size_m }}px;
												{%- if block.settings.desc_margin != blank -%}--vl-t-mg: {{ block.settings.desc_margin }};
												{%- endif -%}{%- if block.settings.desc_margin_m != blank -%} --vl-t-mg-m: {{ block.settings.desc_margin_m }};{% endif %}
											{%- endcapture -%}
											<div class="slideshow-caption__desc vela-text {{ block.settings.desc_font_family }} animate__{{ block.settings.slides_effect }}" style="--animate-duration: {{ time }}s;{{- text_style -}}">
												{{ block.settings.desc }}
											</div>
											{%- assign time = time | plus: 0.3 -%}
										{%- endunless -%}
										{%- unless block.settings.btn_text == blank -%}
											{%- capture button_style -%}
												--vl-b-bd-cl:{{ block.settings.btn_border_color }};--vl-b-bg-cl:{{- block.settings.btn_bg_color -}};--vl-b-cl:{{- block.settings.btn_text_color -}};--vl-b-bd-w: {{ block.settings.btn_border_width }};--vl-b-bd-cl-h:{{ block.settings.btn_border_hover_color }};--vl-b-bg-cl-h:{{ block.settings.btn_bg_hover_color }};--vl-b-cl-h:{{ block.settings.btn_text_hover_color }};--vl-b-fs:{{ block.settings.btn_font_size }}px;--vl-b-fs-m:{{ block.settings.btn_font_size_m }}px;
												{%- if block.settings.btn_padding != blank -%}
													--vl-b-pd:{{ block.settings.btn_padding }};
												{%- endif -%}
												{%- if block.settings.btn_padding_m != blank -%}
													--vl-b-pd-m:{{ block.settings.btn_padding_m }};
												{%- endif -%}
												{%- if block.settings.btn_margin != blank -%}
													--vl-b-mg: {{ block.settings.btn_margin }};
												{%- endif -%}
												{%- if block.settings.btn_margin_m != blank -%}
													--vl-b-mg-m: {{ block.settings.btn_margin_m }};
												{%- endif -%}
											{%- endcapture -%}
											<a href="{{ block.settings.btn_url }}" class="slideshow-caption__btn vela-btn btn animate__{{ block.settings.slides_effect }}" style="--animate-duration: {{ time }}s;{{- button_style -}}">
												<span>{{ block.settings.btn_text }}</span>
											</a>
										{%- endunless -%}
										{%- unless block.settings.btn_text1 == blank -%}
											{%- capture button_style -%}
												--vl-b-bd-cl:{{ block.settings.btn_border_color1 }};--vl-b-bg-cl:{{ block.settings.btn_bg_color1 }};--vl-b-cl:{{ block.settings.btn_text_color1 }};--vl-b-bd-w: {{ block.settings.btn_border_width1 }};--vl-b-bd-cl-h:{{ block.settings.btn_border_hover_color1 }};--vl-b-bg-cl-h:{{ block.settings.btn_bg_hover_color1 }};--vl-b-cl-h:{{ block.settings.btn_text_hover_color1 }};--vl-b-fs:{{ block.settings.btn_font_size1 }}px;--vl-b-fs-m:{{ block.settings.btn_font_size_m1 }}px;
												{%- if block.settings.btn_padding1 != blank -%}
													--vl-b-pd:{{ block.settings.btn_padding1 }};
												{%- endif -%}
												{%- if block.settings.btn_padding_m1 != blank -%}
													--vl-b-pd-m:{{ block.settings.btn_padding_m1 }};
												{%- endif -%}
												{%- if block.settings.btn_margin1 != blank -%}
													--vl-b-mg: {{ block.settings.btn_margin1 }};
												{%- endif -%}
												{%- if block.settings.btn_margin_m1 != blank -%}
													--vl-b-mg-m: {{ block.settings.btn_margin_m1 }};
												{%- endif -%}
											{%- endcapture -%}
											<a href="{{ block.settings.btn_url1 }}" class="slideshow-caption__btn1 vela-btn btn rounded-0 animate__{{ block.settings.slides_effect }}" style="--animate-duration: {{ time }}s;{{- button_style -}}">
												<span>{{ block.settings.btn_text1 }}</span>
											</a>
										{%- endunless -%}
									</div>
								</div>
							</div>
						</div>
					</div>
				{%- endfor -%}
			</div>
			{%- if section.settings.scrolldown -%}
				<div class="slide__scroll position-absolute start-50 bottom-0 translate-middle-x mb-2 mb-md-4 pb-1 pb-md-0">
					<button class="btn-slide__scroll bg-white border-0 d-flex align-items-center rounded-circle position-relative" style="--sl-btn-sd-cl: {{ section.settings.scrolldown_color }}" onclick='scrollBottom1("gotoSlide");' title="{{ section.settings.scrolldown_text }}">
						{% render 'icons', icon: 'down-long', class: "position-absolute top-50 start-50 translate-middle", attr: 'width="20" height="24"' %}
						<svg class="text-circle" viewBox="0 0 100 100" width="95" height="95">
							<defs><path id="circle" d=" M 50, 50 m -37, 0 a 37,37 0 1,1 74,0 a 37,37 0 1,1 -74,0"/></defs>
							<text><textPath xlink:href="#circle">{{ section.settings.scrolldown_text }}</textPath></text>
						</svg>
					</button>
				</div>
			{%- endif -%}		
		</div>
	</div>
</div>
{%- if section.settings.scrolldown -%}
  	<div id="gotoSlide"></div>
    <script > 
        function scrollBottom1(element){   
            var ele = document.getElementById(element);   
            ele.scrollIntoView({behavior: "smooth", block: "start", inline: "nearest"}); 
        } 
    </script> 
{%- endif -%}
{% schema %}
	{
		"name": "Slideshow",
		"tag": "section",
        "class": "velaFramework",
        "disabled_on": {
            "groups": ["header", "footer"]
        },
		"settings": [
			{
                "type": "header",
                "content": "t:sections.global.settings.header_section.content"
            },
            {
                "type": "checkbox",
                "id": "full_with",
                "label": "t:sections.global.settings.full_width.label"
            },
            {
                "type": "text",
                "id": "max_with",
                "label": "t:sections.global.settings.max_with.label",
                "info": "t:sections.global.settings.max_with.info"
            },
            {
                "type": "color",
                "id": "color_bg",
                "label": "t:sections.global.settings.bg_color.label",
                "default": "rgba(0,0,0,0)"
            },
            {
                "type": "text",
                "id": "padding_block",
                "label": "t:sections.global.settings.padding.label",
                "placeholder": "0px 0px",
                "info": "t:sections.global.settings.padding.info"
            },
            {
                "type": "text",
                "id": "padding_block_m",
                "label": "t:sections.global.settings.padding_mobile.label",
                "placeholder": "0px 0px",
                "info": "t:sections.global.settings.padding_mobile.info"
            },
            {
                "type": "text",
                "id": "margin_block",
                "label": "t:sections.global.settings.margin.label",
                "placeholder": "0px 0px"
            },
			{
				"type": "select",
				"id": "border_radius",
				"label": "Border radius",
				"default": "rounded-0",
				"options": [
					{
						"value": "rounded-0",
						"label": "Rounded 0"
					},
					{
						"value": "rounded-1",
						"label": "Rounded 1"
					},
					{
						"value": "rounded-2",
						"label": "Rounded 2"
					},
					{
						"value": "rounded-3",
						"label": "Rounded 3"
					},
					{
						"value": "rounded-pill",
						"label": "Rounded pill"
					},
					{
						"value": "rounded-circle",
						"label": "Rounded circle"
					}
				]
			},
            {
                "type": "header",
                "content": "t:sections.global.settings.header_settings.content"
            },
			{
				"type": "text",
				"id": "slideshow-height",
				"label": "Height",
				"default": "800px",
				"info": "Use for desktop ex: 800px"
			},
			{
				"type": "text",
				"id": "slideshow-theight",
				"label": "Height",
				"default": "600px",
				"info": "Use for Tablets ex: 600px"
			},
			{
				"type": "text",
				"id": "slideshow-mheight",
				"label": "Height",
				"default": "350px",
				"info": "Use for mobile ex: 350px"
			},
			{
				"type": "checkbox",
				"id": "navigation",
				"label": "Show Prev/Next arrows",
				"default": true
			},
			{
				"type": "checkbox",
				"id": "pagination",
				"label": "Show dot indicators",
				"default": true
			},
			{
				"type": "checkbox",
				"id": "autoplay",
				"label": "Autoplay",
				"default": true
			},
			{
				"type": "text",
				"id": "autoplay_speed",
				"label": "Change slides every",
				"info": "Required field number. Ex: 8000",
				"default": "8000"
			},
			{
				"type": "header",
				"content": "Caption"
			},
			{
				"type": "checkbox",
				"id": "caption_full_with",
				"label": "Make section full width"
			},
			{
				"type": "text",
				"id": "caption_max_with",
				"label": "Container max width?(px)",
				"info": "Only used when Caption is full width ex: 1440px"
			},
			{
				"type": "checkbox",
				"id": "position_content",
				"label": "Caption Over Image",
				"default": true,
				"info": "Only mobile"
			},
			{
				"type": "header",
				"content": "Scroll down button"
			},
			{
				"type": "checkbox",
				"id": "scrolldown",
				"label": "Enable?",
				"default": false
			},
			{
				"type": "text",
				"id": "scrolldown_text",
				"label": "Text",
				"default": "Scroll down"
			},
			{
				"type": "color",
				"id": "scrolldown_color",
				"label": "Color",
				"default": "#111111"
			},
			{
				"type": "header",
				"content": "Layout special"
			},
			{
				"type": "checkbox",
				"id": "style_special",
				"label": "Special style",
				"default": false
			},
			{
				"type": "checkbox",
				"id": "slide-show-fullheight",
				"label": "Full screen",
				"default": false
			}
		],
		"blocks": [
			{
				"type": "image",
				"name": "Image slide",
				"settings": [
					{
						"type": "image_picker",
						"id": "image",
						"label": "Image Desktop",
						"info": "Recommend image size: 1920x690"
					},
					{
						"type": "select",
						"id": "bg_align",
						"label": "Image Alignment",
						"default": "center",
						"options": [
							{
								"value": "top",
								"label": "Top"
							},
							{
								"value": "center",
								"label": "Center"
							},
							{
								"value": "bottom",
								"label": "Bottom"
							}
						]
					},
					{
						"type": "url",
						"id": "link",
						"label": "Slide URL"
					},
					{
						"type": "header",
						"content": "Caption box style"
					},
					{
						"type": "text",
						"id": "box_maxwidth",
						"label": "Max width(px)",
						"placeholder": "400px",
						"info": "ex: 400px"
					},
					{
						"type": "text",
						"id": "padding_block",
						"label": "Padding",
						"default": "50px 0",
						"placeholder": "0px 0px",
						"info": "Ex: 0px 0px 0px 0px and use on desktop only"
					},
					{
						"type": "color",
						"id": "content_color",
						"label": "Background Color",
						"default": "#ffffff"
					},
					{
						"type": "range",
						"id": "opacity",
						"label": "t:sections.video.settings.opacity.label",
						"min": 0,
						"max": 100,
						"step": 1,
						"unit": "%",
						"default": 0
					},
					{
						"type": "select",
						"id": "alignment",
						"label": "Caption alignment",
						"default": "justify-content-md-start align-items-md-start",
						"options": [
							{
								"value": "justify-content-md-start align-items-md-start",
								"label": "Top Left"
							},
							{
								"value": "justify-content-md-center align-items-md-start",
								"label": "Top Center"
							},
							{
								"value": "justify-content-md-end align-items-md-start",
								"label": "Top Right"
							},
							{
								"value": "justify-content-md-start align-items-md-center",
								"label": "Middle Left"
							},
							{
								"value": "justify-content-md-center align-items-md-center",
								"label": "Middle Center"
							},
							{
								"value": "justify-content-md-end align-items-md-center",
								"label": "Middle Right"
							},
							{
								"value": "justify-content-md-start align-items-md-end",
								"label": "Bottom Left"
							},
							{
								"value": "justify-content-md-center align-items-md-end",
								"label": "Bottom Center"
							},
							{
								"value": "justify-content-md-end align-items-md-end",
								"label": "Bottom Right"
							}
						]
					},
					{
						"type": "select",
						"id": "text-alignment",
						"label": "Text alignment",
						"default": "center",
						"options": [
							{
								"value": "start",
								"label": "Left"
							},
							{
								"value": "center",
								"label": "Center"
							},
							{
								"value": "end",
								"label": "Right"
							}
						]
					},
					{
						"type": "select",
						"id": "slides_effect",
						"label": "Caption effect",
						"options": [
							{
								"value": "default",
								"label": "Default"
							},
							{
								"value": "backInDown",
								"label": "BackInDown"
							},
							{
								"value": "backInLeft",
								"label": "BackInLeft"
							},
							{
								"value": "backInRight",
								"label": "BackInRight"
							},
							{
								"value": "backInUp",
								"label": "BackInUp"
							},
							{
								"value": "bounceInDown",
								"label": "BounceInDown"
							},
							{
								"value": "bounceInLeft",
								"label": "BounceInLeft"
							},
							{
								"value": "bounceInRight",
								"label": "BounceInRight"
